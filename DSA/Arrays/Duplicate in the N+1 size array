public class Practice {
  public static int duplicate(int nums[]){
    int totsum = 0;
    for(int num : nums){
        totsum += num;
    }
    int n = nums.length;
    int sumofn = n * (n-1)/2;

    return Math.abs(totsum - sumofn);
}
    
    public static void main(String[] args) {
        //Jump Game Part - 01
        int nums[] = {1,3,4,3,2};
       System.out.println(duplicate(nums));
    }
}
